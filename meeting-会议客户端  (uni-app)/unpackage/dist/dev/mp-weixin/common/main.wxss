.login-input-box {
  margin: 50rpx 0;
}
.login-input-box .nickname-item, .login-input-box .tel-item, .login-input-box .code-item {
  margin-bottom: 40rpx;
}
.login-input-box .nickname-item .nickname-input, .login-input-box .nickname-item .tel-input, .login-input-box .nickname-item .code-input, .login-input-box .tel-item .nickname-input, .login-input-box .tel-item .tel-input, .login-input-box .tel-item .code-input, .login-input-box .code-item .nickname-input, .login-input-box .code-item .tel-input, .login-input-box .code-item .code-input {
  height: 80rpx;
  line-height: 80rpx;
  border-bottom: 1rpx solid #ced0d5;
}
.login-input-box .nickname-item .nickname-input .placeholder, .login-input-box .nickname-item .tel-input .placeholder, .login-input-box .nickname-item .code-input .placeholder, .login-input-box .tel-item .nickname-input .placeholder, .login-input-box .tel-item .tel-input .placeholder, .login-input-box .tel-item .code-input .placeholder, .login-input-box .code-item .nickname-input .placeholder, .login-input-box .code-item .tel-input .placeholder, .login-input-box .code-item .code-input .placeholder {
  font-size: 28rpx;
}
.login-input-box .nickname-item .nickname-input :focus, .login-input-box .nickname-item .tel-input :focus, .login-input-box .nickname-item .code-input :focus, .login-input-box .tel-item .nickname-input :focus, .login-input-box .tel-item .tel-input :focus, .login-input-box .tel-item .code-input :focus, .login-input-box .code-item .nickname-input :focus, .login-input-box .code-item .tel-input :focus, .login-input-box .code-item .code-input :focus {
  padding-bottom: 1rpx;
  border-bottom: 4rpx solid #448cfc;
}
.login-input-box .code-item {
  position: relative;
}
.login-input-box .code-item .invisible {
  height: 80rpx;
  line-height: 80rpx;
  position: absolute;
  right: 0;
  padding: 0 20rpx;
  font-size: 40rpx;
  background-color: #FFFFFF;
  top: 50%;
  -webkit-transform: translateY(-30%);
          transform: translateY(-30%);
  z-index: 10;
}
.mask {
  position: fixed;
  left: 0;
  bottom: 0;
  right: 0;
  top: 0;
  background-color: rgba(0, 0, 0, 0.1);
}
/* 隐藏滚动条 */
.hidden_bar ::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
}
/* 20rpx 的间距 */
.line_20 {
  height: 20rpx;
  background-color: #f6f6fc;
}
.line_20.cancel_paddng_30 {
  margin: 0 -30rpx;
}
page {
  height: 100%;
}
view, text, navigator, scroll-view, input {
  box-sizing: border-box;
}
.text_ellipsis {
  /* 显示省略号 */
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}
.body-meeting-title {
  /* 显示省略号 */
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  width: 400rpx;
}
.caret_color {
  caret-color: #448cfc;
}
.tip {
  line-height: 60rpx;
  font-size: 26rpx;
  color: #C8C7CC;
  text-align: center;
}
.font-sty {
  font-weight: bold;
  font-size: 50rpx;
  margin: 30rpx 0;
  color: #333333;
  font-family: Times New Roman, 'serif';
  position: relative;
  text-shadow: 3rpx 3rpx 1rpx #333;
}
.font-sty::before {
  position: absolute;
  content: "";
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-image: linear-gradient(-45deg, #fff 0%, #fff 25%, transparent 25%, transparent 50%, #fff 50%, #fff 75%, transparent 75%, transparent 100%);
  background-size: 5rpx 5rpx;
  z-index: 1;
}
.font-sty::after {
  position: absolute;
  content: attr(data-name);
  top: -4rpx;
  left: -2rpx;
  right: 5rpx;
  bottom: 5rpx;
  color: #333;
  z-index: 2;
  text-shadow: 3rpx 3rpx #fff;
}
.w-picker-header > text:last-child {
  color: #448cfc !important;
}
.top-border-1px {
  position: relative;
}
.top-border-1px::before {
  content: '';
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 1rpx;
  background-color: #eee;
}
.text_2line_ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}
